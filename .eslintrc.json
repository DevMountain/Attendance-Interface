{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "linebreak-style": 0,
    "max-len": 0,
    "react/prop-types": 0,
    "no-undef": 1,
    "comma-dangle": 0,
    "no-unused-vars": 1,
    "jsx-a11y/no-static-element-interactions": 0,
    "keyword-spacing": 0,
    "one-var": 0,
    "react/self-closing-comp": 0,
    "react/prefer-stateless-function": 0,
    "prefer-const": 1,
    "guard-for-in": 0,
    "react/no-unescaped-entities": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "no-nested-ternary": 0,
    "react/sort-comp": 0,
    "arrow-body-style": 0,
    "func-names": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-var": 1,
    "react/destructuring-assignment": 0,
    "react/button-has-type": 0,
    "sort-imports": 0,
    "import/order": 0,
    "jsx-a11y/label-has-for": [
      2,
      {
        "components": [
          "Label"
        ],
        "required": {
          "some": [
            "nesting",
            "id"
          ]
        },
        "allowChildren": false
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "react/forbid-prop-types": 0
  },
  "plugins": [
    "prettier"
  ],
  "globals": {
    "document": true,
    "window": true,
    "it": true,
    "cy": true,
    "describe": true,
    "alert": true,
    "test": true,
    "expect": true
  }
}